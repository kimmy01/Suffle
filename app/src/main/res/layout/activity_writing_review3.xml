    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.Feed.WritingReviewActivity3">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_writingReview3_1"
        android:layout_width="match_parent"
        android:layout_height="78dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <ImageView
            android:id="@+id/imageView_writingReview3_before"
            android:layout_width="16.3dp"
            android:layout_height="16.3dp"
            android:layout_marginStart="14.4dp"
            android:layout_marginTop="42.7dp"
            android:src="@drawable/before"
            android:clickable="true"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_writingReview3_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="39.8dp"
            android:text="리뷰 작성"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_writingReview3_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="320dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="14dp"
            android:clickable="true"
            android:text="등록"
            android:textColor="@color/faded_orange"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/view3"
        style="@style/Divider"
        android:layout_marginTop="78dp"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout_writingReview3_1"
        tools:ignore="MissingConstraints"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_writingReview3_2"
        android:layout_width="match_parent"
        android:layout_height="137dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout_writingReview3_1">

        <ImageView
            android:id="@+id/imageView_writingReview3_image"
            android:layout_width="95dp"
            android:layout_height="95dp"
            android:layout_marginStart="14dp"
            android:layout_marginTop="23dp"
            android:layout_marginBottom="19dp"
            android:src="@drawable/image1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView_writingReview3_flag"
            android:layout_width="11dp"
            android:layout_height="13dp"
            android:layout_marginStart="14dp"
            android:layout_marginTop="51dp"
            android:src="@drawable/iconflag"
            app:layout_constraintStart_toEndOf="@+id/imageView_writingReview3_image"
