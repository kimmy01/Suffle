            android:clickable="true"
            android:gravity="center"
            android:padding="5dp"
            android:text="공부하기 좋은"
            android:textColor="@color/faded_orange"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@id/textView_writingReview3_7"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_writingReview3_9"
            android:layout_width="wrap_content"
            android:layout_height="23dp"
            android:layout_marginStart="10.5dp"
            android:layout_marginTop="46.7dp"
            android:background="@drawable/hashtag_button"
            android:clickable="true"
            android:gravity="center"
            android:padding="5dp"
            android:text="회의하기 좋은"
            android:textColor="@color/brownish_grey"
