<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_feedDetail_1"
        android:layout_width="match_parent"
        android:layout_height="78dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/beforeButton_feedDetail"
            android:layout_width="16.3dp"
            android:layout_height="16.3dp"
            android:src="@drawable/before"
            android:clickable="true"
            android:layout_marginStart="17dp"
            android:layout_marginTop="42.6dp"
            android:backgroundTint="#ffffff"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_feedDetail_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="148dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="147dp"
            android:layout_marginBottom="15dp"
            android:clickable="true"
            android:text="라라브레드"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/shareButton_feedDetail"
            android:layout_width="17.5dp"
            android:layout_height="17.5dp"
            android:layout_marginStart="327.1dp"
            android:layout_marginTop="42.8dp"
            android:layout_marginEnd="15.4dp"
            android:layout_marginBottom="17.5dp"
            android:backgroundTint="#ffffff"
            android:src="@drawable/sharebutton"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e9e9e9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout_feedDetail_1" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_subFeedDetail"
        android:layout_width="match_parent"
        android:layout_height="69dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout_feedDetail_1"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="NotSibling">

        <ImageButton
            android:id="@+id/imageButton_subFeedDetail_profileImage"
            android:layout_width="37dp"
            android:layout_height="37dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="32dp"
            android:background="@drawable/profileimage"
            android:scaleType="fitXY"
            android:src="@drawable/profileimage"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_subFeedDetail_profileName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="61dp"
            android:layout_marginTop="34dp"
            android:text="김주은"
            android:textColor="#292929"
            android:textSize="11sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView_subFeedDetail_recommendation"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginTop="32dp"
            android:layout_marginEnd="15dp"
            android:background="@drawable/recommend"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_subFeedDetail2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout_subFeedDetail">

        <TextView
            android:id="@+id/textView_subFeedDetail_placeName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8.8dp"
            android:layout_marginTop="11dp"
            android:text="라라브레드 / 공릉역 1m"
            android:textColor="@color/faded_orange"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@+id/textView_subFeedDetail_starScore"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView_subFeedDetail_star"
            android:layout_width="9.9dp"
            android:layout_height="9.7dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="14.6dp"
            android:src="@drawable/star"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_subFeedDetail_starScore"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="3.3dp"
            android:layout_marginTop="11dp"
            android:text="4.0"
            android:textColor="#ed7d5d"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@+id/imageView_subFeedDetail_star"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_subFeedDetail_mainText"
            android:layout_width="297dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginTop="8dp"
            android:text="신선한 빵과 브런치 가득, 라라브레드는 반려견 테라스 동반 가능해서
넘 좋다!! 파스타랑 샐러드 JMT"
            android:textColor="@color/black"
            android:textSize="11sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_subFeedDetail_placeName" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_subFeedDetail3"
        android:layout_width="match_parent"
        android:layout_height="184.6dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout_subFeedDetail2">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerView_subFeedDetail_1"
            android:layout_width="match_parent"
            android:layout_height="168.9dp"
            android:layout_marginTop="15.7dp"
            android:clipChildren="false"
            android:clipToPadding="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_subFeedDetail4"
        android:layout_width="match_parent"
        android:layout_height="68.5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout_subFeedDetail3">

        <TextView
            android:id="@+id/textView_subFeedDetail_hashtag1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:background="@drawable/hashtag_button"
            android:gravity="center"
            android:padding="5dp"
            android:text=" 인스타에서 핫한 "
            android:textColor="#676767"
            android:textSize="9sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView_subFeedDetail_hashtag2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:background="@drawable/hashtag_button"
            android:gravity="center"
            android:padding="5dp"
            android:text="떠들기 좋은"
            android:textColor="#676767"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@+id/textView_subFeedDetail_hashtag1"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView_subFeedDetail_hashtag3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:background="@drawable/hashtag_button"
            android:gravity="center"
            android:padding="5dp"
            android:text="공부하기 좋은"
            android:textColor="#676767"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@+id/textView_subFeedDetail_hashtag2"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/imageButton_subFeedDetail_heart"
            android:layout_width="16dp"
            android:layout_height="14.1dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="16.6dp"
            android:background="@drawable/heart"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_subFeedDetail_hashtag1" />

        <TextView
            android:id="@+id/textView_subFeedDetail_heartCount"
            android:layout_width="wrap_content"
            android:layout_height="11dp"
            android:layout_marginStart="34.1dp"
            android:layout_marginTop="18.1dp"
            android:text="13"
            android:textColor="#707070"
            android:textSize="9sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_subFeedDetail_hashtag1" />

        <ImageButton
            android:id="@+id/imageButton_subFeedDetail_reply"
            android:layout_width="16.5dp"
            android:layout_height="17dp"
            android:layout_marginStart="55.2dp"
            android:layout_marginTop="15.2dp"
            android:background="@drawable/replybutton"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_subFeedDetail_hashtag1" />

        <TextView
            android:id="@+id/textView_subFeedDetail_replyCount"
            android:layout_width="wrap_content"
            android:layout_height="11dp"
            android:layout_marginStart="74.9dp"
            android:layout_marginTop="18.1dp"
            android:text="1"
            android:textColor="#707070"
            android:textSize="9sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_subFeedDetail_hashtag1" />

        <TextView
            android:id="@+id/textView_subFeedDetail_commentDetail"
            android:layout_width="wrap_content"
            android:layout_height="17dp"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="15dp"
            android:clickable="true"
            android:text="평가 정보 자세히 보기"
            android:textColor="#ed7d5d"
            android:textSize="11sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#e9e9e9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout_subFeedDetail4" />

    <LinearLayout
        android:id="@+id/constraintLayout_subFeedDetail5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout_subFeedDetail4">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerView_subFeedDetail_reply"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:clipToPadding="false"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            tools:listitem="@layout/item_detail_review_reply" />

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>