<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="35.5dp">

    <ImageView
        android:id="@+id/icon_writingreview1item"
        android:layout_width="17dp"
        android:layout_height="16.2dp"
        android:layout_marginStart="17.1dp"
        android:layout_marginTop="8.6dp"
        android:layout_marginBottom="8.6dp"
        android:src="@drawable/icon1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_writingReview1item_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7.9dp"
        android:layout_marginTop="8.6dp"
        android:layout_marginBottom="8.6dp"
        android:lineSpacingExtra="6sp"
        android:textColor="@color/brownish_grey"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/icon_writingreview1item"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="리틀 파스타" />

    <TextView
        android:id="@+id/textView_writingReview1item_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20.6dp"
        android:layout_marginTop="8.6dp"
        android:layout_marginBottom="8.6dp"
        android:lineSpacingExtra="6sp"
        android:textColor="@color/cool_grey"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="공릉점" />

</androidx.constraintlayout.widget.ConstraintLayout>