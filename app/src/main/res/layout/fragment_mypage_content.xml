<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_mypage_1"
        android:layout_width="match_parent"
        android:layout_height="78dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView_mypage_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="8sp"
            android:text="마이페이지"
            android:textColor="@color/black"
            android:textSize="14sp"
            android:textStyle="bold"
            android:layout_marginTop="39dp"
            android:layout_marginBottom="17dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView_example_hamburger"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginTop="43dp"
            android:layout_marginEnd="15dp"
            android:src="@drawable/hamburgerbutton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/view"
        style="@style/Divider"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout_mypage_1"
        tools:ignore="MissingConstraints"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_mypage_2"
        android:layout_width="match_parent"
        android:layout_height="242dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout_mypage_1"
        tools:layout_editor_absoluteX="0dp">

        <ImageView
            android:id="@+id/imageView_mypage_profile"
            android:layout_width="64.3dp"
            android:layout_height="63.8dp"
            android:src="@drawable/profileimage2"
            android:layout_marginTop="30dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_mypage_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="8sp"
            android:text="변다솔"
            android:textColor="@color/brownish_grey"
            android:textSize="14sp"
            android:layout_marginTop="3.4dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView_mypage_profile" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/button_mypage_modify"
            android:layout_width="48dp"
            android:layout_height="17.7dp"
            android:layout_marginStart="9.1dp"
            android:layout_marginTop="99dp"
            android:clickable="true"
            android:background="@drawable/bg_white_modify_round"
            app:layout_constraintStart_toEndOf="@+id/textView_mypage_name"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imageView_mypage_pencil"
                android:layout_width="9.1dp"
                android:layout_height="9.1dp"
                android:src="@drawable/ic_pencil"
                android:layout_marginStart="10dp"
                android:layout_marginTop="4.9dp"
                android:layout_marginBottom="3.7dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView_mypage_modify"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:lineSpacingExtra="4sp"
                android:text="수정"
                android:textColor="@color/brownish_grey"
                android:textSize="9sp"
                android:layout_marginStart="1.9dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="2.8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@+id/imageView_mypage_pencil"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageView
            android:id="@+id/imageView_mypage_group"
            android:layout_width="18.6dp"
            android:layout_height="11.8dp"
            android:layout_marginStart="55.6dp"
            android:layout_marginTop="144.4dp"
            android:src="@drawable/ic_group_24px"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_mypage_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="4sp"
            android:textColor="@color/brownish_grey"
            android:textSize="9sp"
            android:layout_marginTop="3dp"
            android:layout_marginStart="50.6dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView_mypage_group"
            tools:text="그룹" />

        <TextView
            android:id="@+id/textView_mypage_groupNum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="4sp"
            android:layout_marginTop="3dp"
            android:layout_marginStart="7dp"
            android:text="3"
            android:textColor="@color/brownish_grey"
            android:textSize="8sp"
            app:layout_constraintStart_toEndOf="@+id/textView_mypage_group"
            app:layout_constraintTop_toBottomOf="@+id/imageView_mypage_group" />

        <View
            android:id="@+id/divider_mypage_1"
            style="@style/verticalDivider"
            android:layout_width="1dp"
            android:layout_height="30dp"
            android:layout_marginStart="62dp"
            android:layout_marginTop="144dp"
            android:background="@color/brownish_grey"
            app:layout_constraintStart_toEndOf="@+id/imageView_mypage_group"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView_mypage_friend"
            android:layout_width="11.8dp"
            android:layout_height="11.8dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="144.4dp"
            android:src="@drawable/ic_person_24px"
            app:layout_constraintStart_toEndOf="@+id/divider_mypage_1"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_mypage_friend"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="56dp"
            android:layout_marginTop="3dp"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="4sp"
            android:textColor="@color/brownish_grey"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@+id/divider_mypage_1"
            app:layout_constraintTop_toBottomOf="@+id/imageView_mypage_friend"
            android:text="친구" />

        <TextView
            android:id="@+id/textView_mypage_friendNum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="4sp"
            android:layout_marginTop="3dp"
            android:layout_marginStart="7dp"
            android:text="52"
            android:textColor="@color/brownish_grey"
            android:textSize="8sp"
            app:layout_constraintStart_toEndOf="@+id/textView_mypage_friend"
            app:layout_constraintTop_toBottomOf="@+id/imageView_mypage_friend" />

        <View
            android:id="@+id/divider_mypage_2"
            style="@style/verticalDivider"
            android:layout_width="1dp"
            android:layout_height="30dp"
            android:layout_marginStart="62dp"
            android:layout_marginTop="144dp"
            android:background="@color/brownish_grey"
            app:layout_constraintStart_toEndOf="@+id/imageView_mypage_friend"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView_mypage_post"
            android:layout_width="11.8dp"
            android:layout_height="11.8dp"
            android:layout_marginStart="60dp"
            android:layout_marginTop="144.4dp"
            android:src="@drawable/ic_insert_drive_file_24px"
            app:layout_constraintStart_toEndOf="@+id/divider_mypage_2"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_mypage_post"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="47dp"
            android:layout_marginTop="3dp"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="4sp"
            android:text="게시물"
            android:textColor="@color/brownish_grey"
            android:textSize="9sp"
            app:layout_constraintStart_toEndOf="@+id/divider_mypage_2"
            app:layout_constraintTop_toBottomOf="@+id/imageView_mypage_post" />

        <TextView
            android:id="@+id/textView_mypage_postNum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="4sp"
            android:layout_marginTop="3dp"
            android:layout_marginStart="7dp"
            android:text="7"
            android:textColor="@color/brownish_grey"
            android:textSize="8sp"
            app:layout_constraintStart_toEndOf="@+id/textView_mypage_post"
            app:layout_constraintTop_toBottomOf="@+id/imageView_mypage_post" />

        <Button
            android:id="@+id/button_mypage_mylist"
            android:layout_width="251.8dp"
            android:layout_height="20.6dp"
            android:layout_marginTop="192.1dp"
            android:background="@drawable/bg_button_mylist"
            android:text="나의 리스트"
            android:textColor="@color/white"
            android:textSize="11sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout_mypage_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout_mypage_2"
        tools:layout_editor_absoluteX="16dp">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerView_mypage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>




</androidx.constraintlayout.widget.ConstraintLayout>